{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./resources/components",
          "@hoc": "./resources/hoc",
          "@assets": "./resources/assets",
          "@utils": "./app/utils",
          "@modules": "./app/modules",
          "@stores": "./storage/stores",
          "@routes": "./routes"
        }
      }
    ]
  ]
}
